<scroll-view scroll-y bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll"
	scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" class="topLine">
	<view style="width:200%;height:140%;position:absolute;opacity:1;top:-300%">
		<canvas canvas-id="saveCanvans" style="width:200%;height:140%" />
	</view>
	<image class="bg2 sidnone" src="{{bg1}}"></image>
	<image class="bg1 sidnone" src="{{bg2}}"></image>
	<image class="bg11 sidnone" src="{{mylook}}"></image>
	<view class="cz-view">
		<image bindtap="savePic" src="../../images/img/ic_anniu_save.png"></image>
		<image src="../../images/img/ic_anniu_share.png"></image>
	</view>
</scroll-view>