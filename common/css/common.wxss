/* 小箭头 */
.i-arrow,
.i-arrow-down,
.i-arrow-up,
.i-arrow-right,
.i-arrow-left {
    position: relative;
    display: inline-block;
    width: 30rpx;
    height: 26rpx;
}

.i-arrow:after,
.i-arrow-down:after,
.i-arrow-up:after,
.i-arrow-left:after,
.i-arrow-right:after {
    content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #C8C8CD;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;
}

.i-arrow:after,
.i-arrow-right:after {
    border-width: 2px 2px 0 0;
}

.i-arrow-left:after {
    border-width: 0 0 2px 2px;
}

.i-arrow-up:after {
    border-width: 2px 0 0 2px;
}

.i-arrow-down:after {
    border-width: 0 2px 2px 0;
}