/* 布局 */
.weui-cells {
    margin-top: 10px;
    background-color: #FFFFFF;
    line-height: 1.47058824;
    font-size: 28rpx;
    overflow: hidden;
    position: relative;
}

.weui-cell {
    padding: 20rpx 30rpx;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}

.weui-cell:before {
    content: " ";
    position: absolute;
    bottom: -1rpx;
    right: 0;
    height: 1rpx;
    border-bottom: 1rpx solid #ebebeb;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    left: 15rpx;
    z-index: 2;
}

.weui-cells .weui-cell:last-child:before {
    height: 0;
}

.weui-cell__bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

