/* 按钮 */
.weui-btn-area {
    margin: 10rpx 15rpx 15rpx 15rpx;
}

.button-sp-area {
    margin: 0 auto;
    padding: 15rpx 0;
    width: 60%;
}

.weui-btn {
    position: relative;
    display: block;
    box-sizing: border-box;
    font-size: 28rpx;
    text-align: center;
    text-decoration: none;
    color: #FFFFFF;
    line-height: 90rpx;
    border-radius: 10rpx;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}

.weui-btn_default {
    color: #000000;
}

.weui-btn_primary {
    background-color: #ff5500;
    border: 1px solid #ff5500;
}

.weui-btn_cancel {
    background-color: #ccc;
    border: 1px solid #ccc;
}

.weui-btn_disabled {
    color: rgba(255, 255, 255, 0.4);
}

.weui-btn_warn {
    background-color: #E64340;
}

.weui-btn_plain-default {
    color: #353535;
    border: 1rpx solid #999;
}

.weui-btn_plain-primary {
    color: #ff5500;
    border: 1px solid #ff5500;
}

.weui-btn+.weui-btn {
    margin-top: 15rpx;
}

.weui-btn_disabled.weui-btn_default {
    color: rgba(255, 255, 255, 0.6);
    background-color: #F7F7F7;
}

.weui-btn_default,
.weui-btn_plain-default,
.weui-btn_plain-primary {
    background-color: #fff;
}

.weui-btn_inline {
    display: inline-block;
}

.weui-btn_mid {
    padding: 0 50rpx;
    line-height: 70rpx;
    font-size: 32rpx;
    border-radius: 5rpx;
    min-width: 360rpx;
}

.weui-btn_mini {
    display: inline-block;
    padding: 0 30rpx;
    line-height: 52rpx;
    font-size: 28rpx;
    border-radius: 35rpx;
    min-width: 120rpx;
}

.cart-btn {
    background: -webkit-linear-gradient(left, #ff9900, #ffaa00);
    border-radius: 0;
    border: 0
}

.buy-btn {
    background: -webkit-linear-gradient(left, #ff5500, #ff6500);
    border-radius: 0;
    border: 0
}

.weui-btn_mid::after,
.weui-btn_plain-default::after,
.weui-btn_primary::after,
.weui-btn_disabled::after,
.weui-btn_warn::after,
.cart-btn::after,
.buy-btn::after {
    content: none
}

